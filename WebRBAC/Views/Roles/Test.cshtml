@{ 
    Layout = null;
}
<!DOCTYPE html>
<HTML>
<HEAD>
    <TITLE> ZTREE DEMO </TITLE>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <link href="~/zTree_v3/css/zTreeStyle/zTreeStyle.css" rel="stylesheet" />
    <script src="~/zTree_v3/js/jquery-1.4.4.min.js"></script>
    <script src="~/zTree_v3/js/jquery.ztree.core.js"></script>
    <script src="~/zTree_v3/js/jquery.ztree.excheck.js"></script>
    <SCRIPT LANGUAGE="JavaScript">
       var zTreeObj;
       // zTree 的参数配置，深入使用请参考 API 文档（setting 配置详解）
       var setting = {
           check: {
               enable: true
           },
           data: {
               simpleData: {
                   enable: true,
                   idKey: "id",
                   pIdKey: "pId",
                   rootPId: 0
               }
           }
       };
       // zTree 的数据属性，深入使用请参考 API 文档（zTreeNode 节点数据详解）
       var zNodes = [
        { "id": 1, "pId": 0, "name": "test1", open: true },
        { "id": 11, "pId": 1, "name": "test11", open: true },
        { "id": 12, "pId": 1, "name": "test12", open: true },
        { "id": 111, "pId": 11, "name": "test111", open: true }
       ];

       $(document).ready(function(){
          zTreeObj = $.fn.zTree.init($("#treeDemo"), setting, zNodes);
       });
    </SCRIPT>
</HEAD>
<BODY>
    <div>
        <ul id="treeDemo" class="ztree"></ul>
    </div>
</BODY>
</HTML>